#!/home/vivi/Documents/projects/filesort/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'filesort','console_scripts','filesort'
__requires__ = 'filesort'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('filesort', 'console_scripts', 'filesort')()
    )
